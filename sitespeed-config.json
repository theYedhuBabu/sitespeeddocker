{
    "browsertime": {
        "iterations": 1,
        "browser": "chrome",
        "videoParams": {
            "addTimer": true
        }
    },
    "plugins": {
        "add": [
            "@sitespeed.io/plugin-influxdb"
        ]
    },
    "influxdb": {
        "protocol": "http",
        "host": "influxdb",
        "port": 8086,
        "database": "mysitespeed",
        "version": 2,
        "organisation": "myorg",
        "token": "mytoken"
    },
    "metrics": {
        "add": {
            "coach": {
                "allAdvice": true
            }
        }
    }
}